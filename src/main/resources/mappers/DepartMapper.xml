<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.xxq.filemanager.mapper.DepartMapper">

    <select id="queryDepartAll" resultType="com.xxq.filemanager.entity.DepartEntity">
        select
        id,
        d_name departName,
        amount,
        del_flag delFlag
        from sys_depart

    </select>





</mapper>
